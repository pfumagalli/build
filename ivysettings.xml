<?xml version="1.0" encoding="UTF-8"?>

<ivysettings>

  <settings defaultResolver="libraries" />

  <caches defaultCacheDir="${ivy.cache}" />

  <resolvers>

    <filesystem name="libraries">
      <ivy      pattern="${ivy.libraries}/descriptors/[organisation]/[module]/ivy-[revision].xml" />
      <artifact pattern="${ivy.libraries}/artifacts/[organisation]/[module]/[revision]/[artifact]-[revision](-[type]).[ext]" />
    </filesystem>

    <filesystem name="publications" checksums="">
      <ivy      pattern="${ivy.publications}/[organisation]/[module]/[revision]/ivy-[revision].xml" />
      <artifact pattern="${ivy.publications}/[organisation]/[module]/[revision]/[artifact]-[revision]-[type].[ext]" />
    </filesystem>

  </resolvers>
</ivysettings>